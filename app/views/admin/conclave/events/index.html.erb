<p><%= link_to "Create New",  new_admin_conclave_event_url, {:class => "button add", :title => "Add an event"} %></p>
<table class="tableB">
  <thead>
    <tr>
      <th scope="col">Title</th>
      <th scope="col">Start date</th>
      <th scope="col">Start time</th>
    </tr>
  </thead>
  <tfoot>
    <tr>
      <td colspan="4" class="info_results">
        <span class="info_items"><%= page_entries_info @events %></span>
        <span class="pages_navigation right">
          <%= will_paginate @events, :container => false %>
        </span>
        </td>
    </tr>
  </tfoot>
  <tbody>
    <% @events.each do |event| %>

    <tr<%= cycle(' class="pair"','') -%>>
      <td><strong><%= link_to event.title, edit_admin_conclave_event_url(event), {:title => "Edit event"} -%></strong></td>
      <td><%= event.start_date -%></td>
      <td><%= event.start_time -%></td>
    </tr>

    <% end %>
  </tbody>
</table>
